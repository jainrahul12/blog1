
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
  </tr>
 
  <% @books.each do |book| %>
    <tr>
      <td><%= book.title %></td>
      <td><%= book.text %></td>
      <td><%= link_to 'Show', book_path(book) %></td>

      <td><%= link_to 'edit',"/books/#{book.id}/edit" %></td>
      <td><%= link_to 'Show', book_path(book) %></td>


    </tr>
  <% end %>
</table>
